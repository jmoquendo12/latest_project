<h1> Hello</h1>
<table class="table table-striped table-bordered">
  <thead>
  <td>Full Name</td>
  <td>Phone Number</td>
  <td>Street Address</td>
  <td>Address</td>
  <td>Action</td>
  </thead>
  <% @addresses.each do |address| %>
    <div class="w3-container w3-teal ">
      <div class="container-sm">
        <tr>
          <td><%= address.name %></td>
          <td><%= address.phone_number %></td>
          <td><%= address.street_address %></td>
          <td><%= "#{address.barangay&.name} , #{address.region&.name} , #{address.province&.name} #{address.city_municipality&.name}" %></td>
            <td><%= link_to "Edit", edit_address_path(address), class: "btn btn-secondary" %>
            <%= button_to "Delete", address_path(address), class: "btn btn-danger", method: :delete %></td>
        </tr>
      </div>
  <% end %>
  </div>
</table>
</div>
<%= link_to "Home", members_path, class: "btn btn-primary" %>
<%= link_to "Add", new_address_path, class: "btn btn-primary" %>