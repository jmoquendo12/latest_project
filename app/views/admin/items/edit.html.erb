<h1>Hello</h1>
<%= form_with model: @items, url: admin_item_path, method: :patch, data: { turbo: false } do |list| %>
  <div><%= list.label :image %>
    <%= list.file_field :image %></div>
  <div>
    <%= list.label :name %>
    <%= list.text_field :name %>
  </div>

  <div>
    <%= list.label :quantity %>
    <%= list.text_field :quantity %>
  </div>

  <div>
    <%= list.label :minimum_bets %>
    <%= list.text_field :minimum_bets %>
  </div>

  <div>
    <%= list.label :status %>
    <%= list.select :status, Item.statuses.keys.map { |k| [k.humanize, k] }, { prompt: "Choose Status"} %>
  </div>

  <%= list.submit "Update" %>
<% end %>